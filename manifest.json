{
    "name": "GitHub Dark",
    "version": "1.0.2",
    "author": "Brian Surowiec",
    "description" : "Gives GitHub a dark theme.",
    "content_scripts": [{
        "css": [
            "styles.css",
            "theme.css",
            "edge.css"
        ],
        "matches": [
            "*://github.com/*",
            "*://gist.github.com/*",
            "*://guides.github.com/*",
            "*://help.github.com/*",
            "*://raw.github.com/*",
            "*://status.github.com/*",
            "*://developer.github.com/*"
        ]
    }]
}
